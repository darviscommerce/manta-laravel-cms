{
    "name": "darvis/manta-laravel-cms",
    "description": "Laravel Bootstrap 5 CMS",
    "homepage": "https://darvis.nl",
    "keywords": ["darvis", "laravel", "livewire", "bootstrap", "cms", "users"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arvid de Jong",
            "email": "arvid@darvis.nl",
            "homepage": "https://darvis.nl"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Manta\\LaravelCms\\Providers\\MantaCmsProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Manta\\LaravelCms\\": "/src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/darviscommerce/manta-laravel-cms"
        }
    ],
    "require": {
        "darvis/lemmings": ">=1.0.0",
        "darvis/manta-laravel-pages": "*",
        "php-flasher/flasher-laravel": "*",
        "php-flasher/flasher-toastr-laravel": "*"
    }
}
